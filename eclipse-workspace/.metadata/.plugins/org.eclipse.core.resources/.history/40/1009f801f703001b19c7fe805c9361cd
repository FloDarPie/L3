package TP2;

public class ExpressionPostfixee {
	
	public String expr;
	
	public ExpressionPostfixee(String expr) {
		this.expr=expr;	
	}
	
	public int evaluer() {
		
		String[] a = {"1","2","3","4","5","6","7","8","9","0"};
		int val = 0;
		
		for(Character b : this.expr.toCharArray()) {
			boolean drapeau = false;
			for(String aa : a)
			while(drapeau)
				drapeau = b.toString() == aa;
						
				
			}
			
		}
		
		return 0;
	}

}
